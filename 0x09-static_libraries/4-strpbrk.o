#include "main.h"
#include <stddef.h>

/**
 * _strpbrk - searching a string for any of a set of bytes
 * @s: the string
 * @accept: set of bytes
 *
 * Return: a pointer to the byte in s that matches one of the bytes in accept
 *
 */

char *_strpbrk(char *s, char *accept)
{
	unsigned int iv;
	unsigned int jh;

	for (iv = 0; s[iv] != '\0'; iv++)
	{
		if (s[iv] == accept[jh])
			return (&s[iv]);
	}
	return (NULL);
}